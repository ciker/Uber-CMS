<?xml version="1.0" encoding="utf-8"?><template><pkey><![CDATA[basic_site_auth]]></pkey><hkey><![CDATA[login]]></hkey><description><![CDATA[Login page.]]></description><html><![CDATA[<form method="post" action="./login">
	<div class="ROW">
		<div class="CELL2">
			Username:
		</div>
		<div class="CELL2">
			<input type="text" name="username" value="%USERNAME%" class="TEXTBOX" />
		</div>
		<div class="clear"></div>
	</div>
	<div class="ROW">
		<div class="CELL2">
			Password:
		</div>
		<div class="CELL2">
			<input type="text" name="password" value="%PASSWORD%" class="TEXTBOX" />
		</div>
		<div class="clear"></div>
	</div>
	<div class="ROW">
		<div class="CELL1">
			<input type="submit" value="Login" class="BUTTON" />
		</div>
	</div>
	<div class="ROW">
		<div class="CELL1">
			<a href="./recover" class="BUTTON">Forgotten your password?</a>
			<a href="./recover" class="BUTTON">Register?</a>
		</div>
	</div>
</form>]]></html></template>