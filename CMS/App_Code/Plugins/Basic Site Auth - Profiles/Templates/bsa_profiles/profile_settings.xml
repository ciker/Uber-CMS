			<select name="profile_enabled" class="TEXTBOX">%ENABLED%</select>
		</div>
		<div class="clear"></div>
	</div>
	<div class="ROW">
		<div class="CELL2">
			Frame background URL:
		</div>
		<div class="CELL2">
			<input type="text" name="frame_bg_url" class="TEXTBOX" value="%FRAME_BG_URL%" />
		</div>
		<div class="clear"></div>
	</div>
	<div class="ROW">
		<div class="CELL2">
			Frame background colour:
		</div>
		<div class="CELL2">
			<input type="text" class="TEXTBOX color" name="frame_bg_colour" value="%FRAME_BG_COLOUR%" />
		</div>
		<div class="clear"></div>
	</div>
	<div class="ROW">
		<div class="CELL2">
			Pane background colour:
		</div>
		<div class="CELL2">
