<?xml version="1.0" encoding="utf-8"?><template><pkey><![CDATA[basic_site_auth]]></pkey><hkey><![CDATA[recovery_email_form]]></hkey><description><![CDATA[The initial form displayed to the user.]]></description><html><![CDATA[<form method="post" action="<!--URL-->/recover/email">
	<div class="ROW">
		<div class="CELL2">
			Username:
		</div>
		<div class="CELL2">
			<input type="text" class="TEXTBOX" name="username" />
		</div>
		<div class="clear"></div>
	</div>
	<div class="ROW">
		<div class="CELL2">
			Captcha:
		</div>
		<div class="CELL2">
			<img src="<!--URL-->/captcha" alt="Captcha" title="Captcha" /><br />
			<input type="text" class="TEXTBOX" name="captcha" />
		</div>
		<div class="clear"></div>
	</div>
	<div class="ROW">
		<div class="CELL1">
			<input type="submit" value="Send E-mail" class="BUTTON FR" />
		</div>
		<div class="clear"></div>
	</div>
</form>
%ERROR%]]></html></template>